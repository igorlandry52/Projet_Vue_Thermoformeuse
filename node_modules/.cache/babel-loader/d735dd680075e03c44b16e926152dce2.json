CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KAzCA;AA2CA,IAAA,IA3CA,kBA2CA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,YAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,YAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA;;AACA,WAAA,KAAA;AACA;AAlDA;AA3DA,CAAA","sourcesContent":["<template>\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"temperatures\"\n\t\t:search=\"search\"\n\t\t:single-expand=\"singleExpand\"\n    :expanded.sync=\"expanded\"\n    item-key=\"name\"\n    show-expand\n    sort-by=\"calories\"\n    class=\"elevation-1\"\n  >\n    <template v-slot:top>\n      <v-toolbar flat color=\"white\">\n        <v-toolbar-title>Configuration</v-toolbar-title>\n        <v-divider\n          class=\"mx-4\"\n          inset\n          vertical\n        ></v-divider>\n        <v-spacer></v-spacer>\n\t\t\t\t<v-text-field\n\t\t\t\t\tv-model=\"search\"\n\t\t\t\t\tappend-icon=\"mdi-magnify\"\n\t\t\t\t\tlabel=\"Search\"\n\t\t\t\t\tsingle-line\n\t\t\t\t\thide-details\n\t\t\t\t></v-text-field>\n\t\t\t\t<v-spacer></v-spacer>\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\n          <template v-slot:activator=\"{ on }\">\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Add-config</v-btn>\n          </template>\n          <v-card>\n            <v-card-title>\n              <span class=\"headline\">{{ formTitle }}</span>\n            </v-card-title>\n\n            <v-card-text>\n              <v-container>\n                <v-row>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.name\" label=\"Profil Name\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T1\" label=\"T1(°C)\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T2\" label=\"T2(°C)\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T3\" label=\"T3(°C)\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T4\" label=\"T4(°C)\"></v-text-field>\n                  </v-col>\n\t\t\t\t\t\t\t\t\t<v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T5\" label=\"T5(°C)\"></v-text-field>\n                  </v-col>\n\t\t\t\t\t\t\t\t\t<v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.T6\" label=\"T6(°C)\"></v-text-field>\n                  </v-col>\n                </v-row>\n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n\t\t<template v-slot:expanded-item=\"{ headers, item }\">\n      <td :colspan=\"headers.length\" >More info about {{ item.name }}</td>\n\t\t\t<v-expansion-panels></v-expansion-panels>\n    </template>\n    <template v-slot:item.actions=\"{ item }\" >\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        mdi-delete\n      </v-icon>\n    </template>\n    <template v-slot:no-data>\n      <v-btn color=\"primary\" @click=\"initialize\">Reset</v-btn>\n    </template>\n  </v-data-table>\n</template>\n\n<script>\n  export default {\n    data: () => ({\n\t\t\texpanded: [],\n      singleExpand: true,\n      dialog: false,\n\t\t\tsearch: '',\n      headers: [\n        {\n          text: 'Profil Name',\n          align: 'start',\n          sortable: false,\n          value: 'name',\n        },\n        { text: 'T1(°C)', value: 'T1' },\n        { text: 'T2(°C)', value: 'T2' },\n        { text: 'T3(°C)', value: 'T3' },\n\t\t\t\t{ text: 'T4(°C)', value: 'T4' },\n\t\t\t\t{ text: 'T5(°C)', value: 'T5' },\n\t\t\t\t{ text: 'T6(°C)', value: 'T6' },\n        { text: 'Action', value: 'actions', sortable: false },\n      ],\n      temperatures: [],\n      editedIndex: -1,\n      editedItem: {\n        name: '',\n        T1: 0,\n        T2: 0,\n        T3: 0,\n\t\t\t\tT4: 0,\n\t\t\t\tT5: 0,\n\t\t\t\tT6: 0\n      },\n      defaultItem: {\n        name: '',\n        T1: 0,\n        T2: 0,\n        T3: 0,\n\t\t\t\tT4: 0,\n\t\t\t\tT5: 0,\n\t\t\t\tT6: 0\n      },\n    }),\n\n    computed: {\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\n      },\n    },\n\n    watch: {\n      dialog (val) {\n        val || this.close()\n      },\n    },\n\n    created () {\n      this.initialize()\n    },\n\n    methods: {\n      initialize () {\n        this.temperatures = [\n          {\n            name: 'Jean',\n\t\t\t\t\t\tT1: 180,\n\t\t\t\t\t\tT2: 150,\n\t\t\t\t\t\tT3: 215,\n\t\t\t\t\t\tT4: 220,\n\t\t\t\t\t\tT5: 170,\n\t\t\t\t\t\tT6: 165,\n          },\n          {\n            name: 'Paul',\n            T1: 168,\n\t\t\t\t\t\tT2: 157,\n\t\t\t\t\t\tT3: 210,\n\t\t\t\t\t\tT4: 196,\n\t\t\t\t\t\tT5: 179,\n\t\t\t\t\t\tT6: 188,\n          }\n        ]\n      },\n\n      editItem (item) {\n        this.editedIndex = this.temperatures.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialog = true\n      },\n\n      deleteItem (item) {\n        const index = this.temperatures.indexOf(item)\n        confirm('Are you sure you want to delete this item?') && this.temperatures.splice(index, 1)\n      },\n\n      close () {\n        this.dialog = false\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        })\n      },\n\n      save () {\n        if (this.editedIndex > -1) {\n          Object.assign(this.temperatures[this.editedIndex], this.editedItem)\n        } else {\n          this.temperatures.push(this.editedItem)\n        }\n        this.close()\n      },\n    },\n  }\n</script>"],"sourceRoot":"src/views"}]}